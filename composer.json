{
  "name": "frenzelgmbh/appcommon",
  "description": "Common App Parts for our yii2 projects",
  "keywords": [
    "yii2",
    "smart blog",
    "frenzel gmbh"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/frenzelgmbh/appcommon/issues?state=open",
    "source": "https://github.com/frenzelgmbh/appcommon"
  },
  "authors": [
    {
      "name": "Philipp Frenzel",
      "email": "philipp@frenzel.net",
      "homepage": "http://frenzel.net"
    }
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.horde.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2":"*",
    "philippfrenzel/yii2toolbarbutton":"dev-master",
    "philippfrenzel/yii2tooltipster":"dev-master",
    "philippfrenzel/yiiwymeditor":"dev-master",
    "philippfrenzel/yii2elfinder":"dev-master",
    "philippfrenzel/yii2imageslider":"*",
    "kartik-v/yii2-icons": "dev-master",
    "kartik-v/yii2-widgets": "dev-master",
    "kartik-v/yii2-helpers": "dev-master",
    "kartik-v/yii2-grid": "dev-master",
    "kartik-v/yii2-context-menu": "dev-master",       
    "dektrium/yii2-user":"dev-master",
    "knplabs/gaufrette": "0.2.*@dev",
    "nlp-tools/nlp-tools":"dev-master",
    "yiisoft/yii2-codeception": "*",
    "codeception/specify":"*",
    "codeception/verify":"*",
    "yiisoft/yii2-debug":"*",
    "yiisoft/yii2-gii":"*",
    "schmunk42/yii2-packaii":"*",
    "phpdocumentor/phpdocumentor":"2.*",
    "guzzlehttp/guzzle":"~4.0",
    "subdee/yii2-soap-server": "*"
  },
  "autoload": {
    "psr-4": {
      "frenzelgmbh\\appcommon\\": ""
    }
  }
}
